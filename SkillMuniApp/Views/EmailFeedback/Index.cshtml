
@{
    ViewBag.Title = "Feedback";
    Layout = "~/Views/Shared/_cms_Layout.cshtml";
    List<m2ostnext.Models.Email_feedback_cms> feedback = (List<m2ostnext.Models.Email_feedback_cms>)ViewData["feedback"];
}


<link rel="stylesheet" href="~/Content/datatable/css/jquery.dataTables.min.css" />
<link rel="stylesheet" href="~/Content/datatable/css/dataTables.bootstrap.min.css" />
<script type="text/javascript" src="~/Content/datatable/js/jquery.dataTables.min.js"></script>

<script>
    $(document).ready(function () {
        $("#report-table").dataTable();
    });
</script>
<div class="content">
    <div class="col-md-12 column">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h6 class="panel-title">Email feedback user assignment</h6>
            </div>
            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <a style="background: #018ccf; color: white;margin-top: 13px;margin-left: -177px;" href="@Url.Action("add_email_feedback", "EmailFeedback")" class="btn btn-default">Create User</a>
                </div>
            </div>
            <hr/>
            <div class="panel-body">
                <div class="row">
                    <table id="report-table" class="table table-striped  table-bordered dataTable sm" width="100%" border="0" cellspacing="0" cellpadding="0">
                        <thead>
                            <tr>
                                <th>
                                    User Name
                                </th>
                                <th>
                                    Email Id
                                </th>
                                <th>
                                    Role
                                </th>
                                <th>
                                    Edit
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                        @foreach (var item in feedback)
                        {
                            <tr>
                                <td>
                                    @item.emp_name
                                </td>
                                <td>
                                    @item.emp_emailID
                                </td>
                                <td>
                                    @item.data
                                </td>
                                <td>
                                    <a href="@Url.Action("editEmailFeedback","EmailFeedback",new { emp_id = item.emp_id})" target="_blank">
                                        <i class="glyphicon glyphicon-edit"></i>
                                    </a>
                                </td>
                            </tr>

                        }
                    </tbody>

                    </table>

                </div>
            </div>
        </div>
    </div>
</div>






