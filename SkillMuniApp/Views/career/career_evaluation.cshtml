    function select_file(btn, imgf, ban) {

        var file = $("#" + btn + "")[0].files[0];
        var filename = "";
        if (file) {
            filename = file.name;
            $("#" + ban + "").addClass("validate[required,custom[url]]");
        } else {
            $("#" + ban + "").removeClass("validate[required,custom[url]]");
        }

        $("#" + imgf + "").val(filename);
