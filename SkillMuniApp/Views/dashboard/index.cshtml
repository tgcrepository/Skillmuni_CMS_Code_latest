                            {
                                <tr style="font-size: 12px;">
                                    <td>@item.tile_name</td>
                                    <td>@item.category_tile</td>
                                    <td>@item.brief_category</td>
                                    <td>@item.brief_title</td>
                                </tr>
                            }
