                            <tr>
                                <th>Name</th>
                                <th>Contact No</th>
                                <th>Email</th>
                                <th>College</th>
                            </tr>
                        </thead>
                        <tbody id="bodyApp" style="font-size: 12px;"></tbody>
                    </table>
                </div>
            </div>
        </div>
