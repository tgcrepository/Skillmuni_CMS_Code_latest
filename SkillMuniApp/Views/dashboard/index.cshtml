    <div class="row">
        <div class="col-md-8">

            <!-- TABLE: LATEST ORDERS -->
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Top 5 Location Registered Users</h3>
                    <div class="box-tools pull-right">
                        <button type="button" class="btn btn-box-tool" data-widget="collapse">
                            <i class="fa fa-minus"></i>
                        </button>
                        <button type="button" class="btn btn-box-tool" data-widget="remove"><i class="fa fa-times"></i></button>
                    </div>
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <div id="chartdiv"></div>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">Brief Reports</h3>
                    <div class="box-tools pull-right">
                        <button type="button" class="btn btn-box-tool" data-widget="remove"><i class="fa fa-times"></i></button>
                    </div>
                </div>
                <!-- Info Boxes Style 2 -->
                @foreach (var item in dbp)
                {
                    if (cl == 1)
                    {
                        bgcolor = "bg-yellow";
                        iconF = "fa-book";
                        wordB = "completed";
                    }
                    else if (cl == 2)
                    {
                        bgcolor = "bg-green";
                        iconF = "fa-clock-o";
                        wordB = "Pending";
                    }
                    else if (cl == 3)
                    {
                        bgcolor = "bg-red";
                        iconF = "fa-trash-o";
                        wordB = "Deleted";
                    }
                    <div class="info-box @bgcolor">
                        <span class="info-box-icon"><i class="fa @iconF" aria-hidden="true"></i></span>
                        <div class="info-box-content">
                            <span class="info-box-text">@item.Status</span>
                            <span class="info-box-number">@item.status_count</span>
                            <div class="progress">
                                <div class="progress-bar" style="width: @item.percentage%"></div>
                            </div>
                            <span class="progress-description">
                                @item.percentage% @wordB
                            </span>
                        </div>
                        <!-- /.info-box-content -->
                    </div>
                    cl++;
                }
            </div>
            <!-- /.info-box -->
            @*<div class="info-box bg-aqua">
