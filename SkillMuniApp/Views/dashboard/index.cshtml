<script>
    function changeFlag() {
        var ft = $("#flagType").val();
        var data = @Html.Raw(Json.Encode(tcp));
        $('#leadTbl').dataTable().fnDestroy();
        $("#bodyApp").html("");
        if (ft == "tpo") {
            var str = '';
            for (i = 0; i < data.length; i++)
            {
                if (data[i].form_flag == "tpo") {
                    str += '<tr><td>' + data[i].name + '</td><td>' + data[i].contact_number + '</td><td>' + data[i].email + '</td><td>' + data[i].college_name + '</td></tr>';
                }
            }            
            $("#bodyApp").append(str);
             dataTable_func();
        }
        else {
            var str = '';
            for (i = 0; i < data.length; i++)
            {
                if (data[i].form_flag == "corp") {
                    str += '<tr><td>' + data[i].name + '</td><td>' + data[i].contact_number + '</td><td>' + data[i].email + '</td><td>' + data[i].college_name + '</td></tr>';
                }
            }            
            $("#bodyApp").append(str);
            dataTable_func();
        }
    }

    function initTable() {
         var ft = $("#flagType").val();
        var data = @Html.Raw(Json.Encode(tcp));
        $('#leadTbl').dataTable().fnDestroy();
        $("#bodyApp").html("");
        if (ft == "tpo") {
            var str = '';
            for (i = 0; i < data.length; i++)
            {
                if (data[i].form_flag == "tpo") {
                    str += '<tr><td>' + data[i].name + '</td><td>' + data[i].contact_number + '</td><td>' + data[i].email + '</td><td>' + data[i].college_name + '</td></tr>';
                }
            }            
            $("#bodyApp").append(str);
             dataTable_func();
        }
    }

    $(document).ready(function () {
        initTable();        
         $('#tileRep').DataTable({
            dom: 'Bfrtip',
            buttons: [
                //'copy', 'csv', 'excel', 'pdf', 'print'
                'csv', 'excel'
            ]
        });
    });

    function dataTable_func() {        
         $('#leadTbl').DataTable({
            dom: 'Bfrtip',
            buttons: [
                //'copy', 'csv', 'excel', 'pdf', 'print'
                'csv', 'excel'
            ]
         });         
    }

    function getTile() {
        var aId = $("#academicId").val();

        $.ajax({
            url: '@Url.Action("tile_report_json","Dashboard")',     
            type: "GET",
            data: { "acadId" : aId }, 
            success: function (data) {
                if (data.ditr.length > 0) {
                    $('#tileRep').dataTable().fnDestroy();
                    $("#tileTBody").html("");
                    var str = '';
                    for (i = 0; i < data.ditr.length; i++) {
                        str += '<tr style="font-size: 12px;"><td>' + data.ditr[i].tile_name + '</td><td>' + data.ditr[i].category_tile + '</td><td>' + data.ditr[i].brief_category + '</td><td>' + data.ditr[i].brief_title + '</td></tr>'
                    }                    
                    $("#tileTBody").append(str);
                    tileReportDatatable();
                }
                if (data.tbct.length > 0) {                    
                    var str = '';
                    str += '<option value="0" selected>All</option>';
                    for (i = 0; i < data.tbct.length; i++) {
                        str += '<option value="'+data.tbct[i].id_brief_category_tile+'">' + data.tbct[i].category_tile + '</option>';
                    }
                    $("#tileId").html(str);
                }



                    }
                });
    }

    function getTileValues() {
        var aId = $("#academicId").val();
        var tId = $("#tileId").val();        
        $.ajax({
            url: '@Url.Action("academic_tile_dashboard_report","Dashboard")',
            type: "GET",
            data: { "acadId": aId, "tileId": tId },
            success: function (data) {
                if (data.length > 0) {
                      $('#tileRep').dataTable().fnDestroy();
                    $("#tileTBody").html("");
                    var str = '';
                    for (i = 0; i < data.length; i++) {
                        str += '<tr style="font-size: 12px;"><td>' + data[i].tile_name + '</td><td>' + data[i].category_tile + '</td><td>' + data[i].brief_category + '</td><td>' + data[i].brief_title + '</td></tr>'
                    }                    
                    $("#tileTBody").append(str);
                    tileReportDatatable();
                }
            }
        });
    }

    function tileReportDatatable() {
        $('#tileRep').DataTable({
            dom: 'Bfrtip',
            buttons: [
                //'copy', 'csv', 'excel', 'pdf', 'print'
                'csv', 'excel'
            ]
        });
    }

</script>