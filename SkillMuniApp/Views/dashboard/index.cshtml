                    <p>Total Number of Briefs</p>
                </div>
                <div class="icon">
                    <i class="glyphicon glyphicon-book"></i>
                </div>
                <div class="small-box-footer">
                    <span style="float: left;margin-left: 10%;">Active : @Activebrief</span>
                    <span>Inactive : @InActivebrief</span>
                </div>
            </div>
        </div>
        <!-- ./col -->
        <div class="col-lg-3 col-xs-6">
            <!-- small box -->
            <div class="small-box bg-yellow">
                <div class="inner">
                    <h3>@totalCat</h3>
                    <p>Total Number of categories</p>
                </div>
                <div class="icon">
                    <i class="glyphicon glyphicon-tags"></i>
                </div>
                <div class="small-box-footer">
                    <span style="float: left;margin-left: 10%;">Active : @Activecat</span>
                    <span>Inactive : @InActivecat</span>
                </div>
            </div>
        </div>
        <!-- ./col -->
        <div class="col-lg-3 col-xs-6">
