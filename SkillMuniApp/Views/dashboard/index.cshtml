                    <h3>@usercount</h3>
                    <p>Number of registered users</p>
                </div>
                <div class="icon">
                    <i class="glyphicon glyphicon-user"></i>
                </div>
                <div class="small-box-footer">
                    <span style="float: left;margin-left: 10%;">Active : @ActiveUser</span>
                    <span>Inactive : @InActiveUser</span>
                </div>
            </div>
        </div>
        <!-- ./col -->
        <div class="col-lg-3 col-xs-6">
