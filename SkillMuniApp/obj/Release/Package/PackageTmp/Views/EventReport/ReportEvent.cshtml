
@{
    ViewBag.Title = "ReportEvent";
    Layout = "~/Views/Shared/_cms_Layout.cshtml";
    List<m2ostnext.Models.Event> events = (List<m2ostnext.Models.Event>)ViewData["events"];
}

<script type="text/javascript" src="https://cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js" defer></script>
<link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.8/css/jquery.dataTables.min.css">
<link rel="stylesheet" type="text/css" href="~/Content/studentreport/StyleSheet1.css">
<script type="text/javascript" src="~/Content/studentreport/pagination.js"></script>

<style>
    #manja_wrapper {
        overflow: scroll;
    }

    #manja_filter input {
        border: 1px solid #ccc;
    }
</style>

<div class="content">
    <div class="col-md-12 column">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h6 class="panel-title">EVENT Report</h6>
            </div>
            <div class="panel-body">
                <div class="row">
                    <table id="manja" class="table table-striped table-bordered dataTable sm" width="100%" border="0" cellspacing="0" cellpadding="0">
                        <thead>
                            <tr>
                                <th>
                                    Event Title
                                </th>
                                <th>
                                    Event Objective
                                </th>
                                <th>
                                    Event Logo
                                </th>

                                <th>
                                    Event Registration Needed
                                </th>
                                <th>
                                    Event Start Date
                                </th>

                                <th>
                                    Event End Date
                                </th>
                                <th>
                                    Event Duration
                                </th>
                                <th>
                                    Event Location
                                </th>
                                <th>
                                    Detailed Address
                                </th>
                                <th>
                                    Closed Event
                                </th>
                                <th>
                                    No of participants
                                </th>
                                <th>
                                    Contact Name
                                </th>
                                <th>
                                    Contact Number
                                </th>
                                <th>
                                    id_user
                                </th>
                                <th>
                                    User Name
                                </th>
                                <th>
                                    User Contact Number
                                </th>
                                <th>
                                    User Email ID
                                </th>
                                <th>
                                    User Registration Date Time Stamp

                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in events)
                            {
                                <tr>

                                    <td>
                                        @item.event_title
                                    </td>

                                    <td>
                                        @item.event_objective
                                    </td>

                                    <td>
                                        @item.event_logo
                                    </td>
                                    <td>
                                        @item.is_registration_needed
                                    </td>
                                    <td>
                                        @item.event_start_date
                                    </td>

                                    <td>
                                        @item.event_end_date
                                    </td>

                                    <td>
                                        @item.event_duration
                                    </td>
                                    <td>
                                        @item.location_text
                                    </td>
                                    <td>
                                        @item.address
                                    </td>
                                    <td>
                                        @item.is_event_closed
                                    </td>
                                    <td>
                                        @item.user_count
                                    </td>
                                    <td>
                                        @item.contact_name

                                    </td>
                                    <td>
                                        @item.contact_number
                                    </td>
                                    <td>
                                        @item.UID
                                    </td>
                                    <td>
                                        @item.FIRSTNAME
                                    </td>
                                    <td>
                                        @item.MOBILE
                                    </td>
                                    <td>
                                        @item.EMAIL
                                    </td>
                                    <td>
                                        @item.updated_date_time
                                    </td>
                                </tr>

                            }
                        </tbody>

                    </table>
                    <a href='@Url.Action("ExcelAllReport", "EventReport")' class="btn btn-primary">Report Download</a>

                </div>
            </div>
        </div>
    </div>
</div>



