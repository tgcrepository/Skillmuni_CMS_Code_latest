
@{
    ViewBag.Title = "Special_points_dashboard";
    Layout = "~/Views/Shared/_cms_Layout.cshtml";
    List<m2ostnext.Models.tbl_university_special_points_master> SpMaster = (List<m2ostnext.Models.tbl_university_special_points_master>)ViewData["SpMaster"];
}

<div class="content">
    <div class="col-md-12 column">
        <div class="panel panel-default m2ost_info">
            <div class="panel-heading m2ost_info_hat">
                <span>Special Metric Dashboard</span>
            </div>
            <div class="panel-body">
                <div class="form-horizontal">
                    <div id="div-kpi-data-grid" class="relegatGrid" style="margin-bottom: 1%;">
                        <table class="table" style="height:80px;width: 100%;">
                            <thead>
                                <tr style="background-color:#367fa9;color:white">
                                    <th>Special Value Name</th>
                                    <th>Special Value Description</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody class="prog-table" id="specMetricTbl">
                                @foreach (var item in SpMaster)
                                {
                                    <tr>
                                        <td>@item.special_value_name</td>
                                        <td>@item.special_value_description</td>
                                        <td><a class="btn btn-primary" href="@Url.Action("Special_Points_edit","universityKPI",new { id_sp = item.id_special_points})"><i class="fa fa-pencil" aria-hidden="true"></i>Edit</a>@*<a class="btn btn-primary"><i class="fa fa-trash" aria-hidden="true"></i>Delete</a>*@</td>
                                    </tr>

                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

